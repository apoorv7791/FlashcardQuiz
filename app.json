{
  "expo": {
    "name": "flashcard-quiz",
    "slug": "flashcard-quiz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/quiz.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "sdkVersion": "53.0.0",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.flashcardquiz"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/quiz.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.flashcardquiz"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/quiz.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7867348b-7eac-4ffd-aec2-f7b82bffcfa6"
      }
    }
  }
}